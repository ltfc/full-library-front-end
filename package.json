{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/test": "^3.2.16",
    "antd": "^4.5.4",
    "enquire-js": "^0.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rc-banner-anim": "^2.4.4",
    "rc-queue-anim": "^1.8.5",
    "rc-scroll-anim": "^2.7.4",
    "rc-texty": "^0.2.0",
    "rc-tween-one": "^2.7.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-sublime-video": "^0.2.5",
    "umi": "^3.2.16",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-react": "^1.5.22"
  }
}
